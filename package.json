{
  "name": "modul10_1",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:html",
    "start": "run-p start:*",
    "start:webpack": "webpack --display-error-details",
    "start:babel": "babel js/script.js --watch --out-file js/build.js",
    "test:html": "nu-html-checker *.html",
    "init-project": "npm install && npm-run-all init:*",
    "init:dirs": "mkdirp sass css vendor images js",
    "init:files": "touch README.md css/style.css sass/style.scss js/script.js js/app.js js/world.js",
    "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
    "build": "npm-run-all build:* test",
    "build:clean": "rimraf -r css/*.css",
    "build:sass": "node-sass --output-style compact -o css sass",
    "build:autoprefixer": "autoprefixer-cli css/style.css",
    "build-dev:sass": "node-sass --output-style expanded --source-map true -o css sass",
    "watch": "npm-run-all -p watch:* ",
    "watch:browser-sync": "browser-sync start --server --files css/*.css *.html",
    "watch:sassprefixer": "onchange -d 100 sass -- npm-run-all -s build-dev:sass build:autoprefixer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastianbukowski/Modul10_1.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sebastianbukowski/Modul10_1/issues"
  },
  "homepage": "https://github.com/sebastianbukowski/Modul10_1#readme",
  "devDependencies": {
    "autoprefixer-cli": "^1.0.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.26.3",
    "http-server": "^0.11.1",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.10.0",
    "npm-run-all": "^4.1.3",
    "nu-html-checker": "^0.1.0",
    "onchange": "^5.1.0",
    "rimraf": "^2.6.2",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "normalize.css": "^8.0.1"
  }
}
